        xmlns:local="clr-namespace:XamlOfClientSide"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400">
    
    <Grid>


        <ComboBox x:Name="ComboBoxOfCommandType" HorizontalAlignment="Left" Margin="251,109,0,0" VerticalAlignment="Top" Width="120"/>
