        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Margin="251,73,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="Run" HorizontalAlignment="Left" Margin="271,158,0,0" VerticalAlignment="Top" Width="80" Click="RunButton_Click"/>
        <ListBox x:Name="ListBox" d:ItemsSource="{d:SampleData ItemCount=9}" Margin="0,32,171,217"/>




    </Grid>
